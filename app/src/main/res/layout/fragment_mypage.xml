<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/ic_mydaangn"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="13dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/ic_mydaangn_setting_outline_24" />

    <androidx.cardview.widget.CardView
        android:id="@+id/user_profile_image"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="60dp"
        android:backgroundTint="#AAAAAA"
        app:cardElevation="0dp"
        app:cardCornerRadius="100dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="노나니노니"
        android:textSize="18dp"
        android:fontFamily="@font/inter_bold"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/user_profile_image"
        app:layout_constraintTop_toTopOf="@+id/user_profile_image"
        app:layout_constraintBottom_toBottomOf="@id/user_profile_image"
        android:layout_marginStart="11dp"/>



    <androidx.cardview.widget.CardView
        android:id="@+id/frame_64"
        android:layout_width="80dp"
        android:layout_height="29dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="61dp"
        android:backgroundTint="#F2F3F7"
        android:elevation="0dp" />

    <TextView
        android:id="@+id/profile_lookiing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="11dp"
        android:text="프로필 보기"
        android:fontFamily="@font/inter_bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/frame_64"
        app:layout_constraintEnd_toEndOf="@id/frame_64"
        android:elevation="10dp"
        android:layout_marginEnd="13dp"
        android:layout_marginTop="8dp"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/carrotpay_charge_withdraw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/user_profile_image">
        <ImageView
            android:id="@+id/rectangle_cash"
            android:layout_width="300dp"
            android:layout_height="106dp"
            android:src="@drawable/rectangle_style_large"
            app:layout_constraintTop_toTopOf="@id/carrotpay_charge_withdraw"
            android:layout_marginTop="14dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="15dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="14dp"/>

        <ImageView
            android:id="@+id/onbordong_logo"
            android:layout_width="56dp"
            android:layout_height="14dp"
            android:src="@drawable/onboarding_logo"
            app:layout_constraintTop_toTopOf="@id/rectangle_cash"
            android:layout_marginTop="25dp"
            app:layout_constraintStart_toStartOf="@id/rectangle_cash"
            android:layout_marginStart="18dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12dp"
            android:textColor="@color/black"
            android:text="당근하는 새로운 방법, 당근페이!"
            android:fontFamily="@font/inter_regular"
            app:layout_constraintTop_toTopOf="@id/rectangle_cash"
            app:layout_constraintStart_toEndOf="@+id/onbordong_logo"
            android:layout_marginTop="20dp"
            android:layout_marginStart="14dp"/>
        <ImageView
            android:id="@+id/icon_post_menu"
            android:layout_width="9dp"
            android:layout_height="15dp"
            android:src="@drawable/icon_post_article_more"
            app:layout_constraintTop_toTopOf="@+id/rectangle_cash"
            app:layout_constraintEnd_toEndOf="@id/rectangle_cash"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="14dp"/>
        <androidx.cardview.widget.CardView
            android:id="@+id/frame_charge"
            android:layout_width="120dp"
            android:layout_height="33dp"
            android:backgroundTint="@color/white_gray"
            app:layout_constraintStart_toStartOf="@id/rectangle_cash"
            app:layout_constraintBottom_toBottomOf="@id/rectangle_cash"
            android:layout_marginStart="18dp"
            android:layout_marginBottom="15dp"
            android:elevation="0dp" >
            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@drawable/tab_icon_write_gb_n"
                android:layout_marginTop="9.5dp"
                android:layout_marginStart="35dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="충전"
                android:fontFamily="@font/inter_bold"
                android:textColor="@color/black"
                android:textSize="13dp"
                android:layout_marginStart="53dp"
                android:layout_marginTop="6dp"/>
        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:id="@+id/frame_withdraw"
            android:layout_width="120dp"
            android:layout_height="33dp"
            android:backgroundTint="@color/white_gray"
            app:layout_constraintEnd_toEndOf="@id/rectangle_cash"
            app:layout_constraintBottom_toBottomOf="@id/rectangle_cash"
            android:layout_marginEnd="18dp"
            android:layout_marginBottom="15dp"
            android:elevation="0dp">

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@drawable/tab_icon_write_gb_n"
                android:layout_marginTop="9.5dp"
                android:layout_marginStart="32dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="계좌송금"
                android:textSize="13dp"
                android:fontFamily="@font/inter_bold"
                android:textColor="@color/black"
                android:layout_marginStart="50dp"
                android:layout_marginTop="6dp"/>

        </androidx.cardview.widget.CardView>




    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/visit_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/carrotpay_charge_withdraw"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <TextView
            android:id="@+id/visit_recent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/visit_list"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            android:layout_marginTop="15dp"
            android:textColor="@color/black"
            android:fontFamily="@font/inter_bold"
            android:text="최근 방문"
            android:textSize="14dp"/>
        <ImageView
            android:id="@+id/visit_all"
            android:layout_width="68dp"
            android:layout_height="34dp"
            android:src="@drawable/rectangle_style_small"
            android:layout_marginTop="19dp"
            app:layout_constraintTop_toBottomOf="@id/visit_recent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="12dp"/>
        <ImageView
            android:id="@+id/visit_all_category"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:src="@drawable/tab_icon_category_n"
            app:layout_constraintStart_toStartOf="@id/visit_all"
            android:layout_marginTop="9dp"
            app:layout_constraintTop_toTopOf="@id/visit_all"
            android:layout_marginStart="10dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="전체"
            android:textSize="13dp"
            android:fontFamily="@font/inter_regular"
            android:textColor="@color/black"
            app:layout_constraintStart_toEndOf="@id/visit_all_category"
            app:layout_constraintTop_toTopOf="@id/visit_all"
            android:layout_marginTop="5.5dp"
            android:layout_marginStart="4dp"/>



        <ImageView
            android:id="@+id/male_fashion"
            android:layout_width="99dp"
            android:layout_height="34dp"
            android:src="@drawable/rectangle_style_small"
            android:layout_marginTop="19dp"
            app:layout_constraintTop_toBottomOf="@id/visit_recent"
            app:layout_constraintStart_toEndOf="@id/visit_all"
            android:layout_marginStart="12dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13dp"
            android:text="남성패션/잡화"
            android:fontFamily="@font/inter_regular"
            android:textColor="@color/black"
            app:layout_constraintTop_toTopOf="@id/male_fashion"
            app:layout_constraintStart_toStartOf="@id/male_fashion"
            android:layout_marginStart="8.5dp"
            android:layout_marginTop="5.5dp"/>

        <ImageView
            android:id="@+id/female_fashion"
            android:layout_width="73dp"
            android:layout_height="34dp"
            android:src="@drawable/rectangle_style_small"
            android:layout_marginTop="19dp"
            app:layout_constraintTop_toBottomOf="@id/visit_recent"
            app:layout_constraintStart_toEndOf="@id/male_fashion"
            android:layout_marginStart="12dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13dp"
            android:text="여성의류"
            android:fontFamily="@font/inter_regular"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="@id/female_fashion"
            app:layout_constraintTop_toTopOf="@id/female_fashion"
            android:layout_marginTop="5.5dp"
            android:layout_marginStart="12dp"/>

        <ImageView
            android:id="@+id/furniture"
            android:layout_width="99dp"
            android:layout_height="34dp"
            android:src="@drawable/rectangle_style_small"
            android:layout_marginTop="19dp"
            app:layout_constraintTop_toBottomOf="@id/visit_recent"
            app:layout_constraintStart_toEndOf="@id/female_fashion"
            android:layout_marginStart="12dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13dp"
            android:text="가구/인테리어"
            android:fontFamily="@font/inter_regular"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="@id/furniture"
            app:layout_constraintTop_toTopOf="@id/furniture"
            android:layout_marginTop="5.5dp"
            android:layout_marginStart="12dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/trade_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/visit_list"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:padding="12dp">
        
        <TextView
            android:id="@+id/tv_my_trade"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="나의 거래"
            android:textColor="@color/black"
            android:fontFamily="@font/inter_bold"
            android:textSize="14dp"
            app:layout_constraintTop_toTopOf="@id/trade_list"
            app:layout_constraintStart_toStartOf="@id/trade_list"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tv_my_trade"
            app:layout_constraintStart_toStartOf="@id/trade_list"
            app:layout_constraintBottom_toBottomOf="@id/trade_list">

            <ImageView
                android:id="@+id/ic_feed_interest_outline"
                android:layout_width="21dp"
                android:layout_height="21dp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="15dp"
                android:src="@drawable/ic_feed_interest_outline_18"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/tv_interest_list"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14dp"
                android:text="관심 목록"
                android:fontFamily="@font/inter_regular"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/ic_feed_interest_outline"
                app:layout_constraintTop_toTopOf="@id/ic_feed_interest_outline"
                android:layout_marginTop="-2dp"
                android:layout_marginStart="5dp"/>

            <ImageView
                android:id="@+id/ic_community_outline"
                android:layout_width="21dp"
                android:layout_height="21dp"
                app:layout_constraintTop_toBottomOf="@id/ic_feed_interest_outline"
                android:layout_marginTop="20dp"
                android:src="@drawable/ic_community_outline_m"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/tv_trade_history"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14dp"
                android:text="판매내역"
                android:fontFamily="@font/inter_regular"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/ic_community_outline"
                app:layout_constraintTop_toTopOf="@id/ic_community_outline"
                android:layout_marginTop="-2dp"
                android:layout_marginStart="5dp"/>
            <ImageView
                android:id="@+id/ic_voucher_outline"
                android:layout_width="21dp"
                android:layout_height="21dp"
                app:layout_constraintTop_toBottomOf="@id/ic_community_outline"
                android:layout_marginTop="20dp"
                android:src="@drawable/ic_voucher_outline_m"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/tv_purchase_history"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14dp"
                android:text="구매내역"
                android:fontFamily="@font/inter_regular"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/ic_voucher_outline"
                app:layout_constraintTop_toTopOf="@id/ic_voucher_outline"
                android:layout_marginTop="-2dp"
                android:layout_marginStart="5dp"/>

            <ImageView
                android:id="@+id/btn_favorite_text_for_post"
                android:layout_width="21dp"
                android:layout_height="21dp"
                app:layout_constraintTop_toBottomOf="@id/ic_voucher_outline"
                android:layout_marginTop="20dp"
                android:src="@drawable/btn_favorite_text_for_post"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/tv_used_trade_account_book"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14dp"
                android:text="중고거래 가계부"
                android:fontFamily="@font/inter_regular"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/btn_favorite_text_for_post"
                app:layout_constraintTop_toTopOf="@id/btn_favorite_text_for_post"
                android:layout_marginTop="-2dp"
                android:layout_marginStart="5dp"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
        
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:id="@+id/line_trade_life"
        android:layout_width="wrap_content"
        android:layout_height="2dp"
        android:background="@color/white_gray"
        app:layout_constraintTop_toBottomOf="@id/trade_list"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_neighborhood_life"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/line_trade_life"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:padding="12dp">
        
        <TextView
            android:id="@+id/tv_my_neighborhood_life"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="나의 동네생활"
            android:fontFamily="@font/inter_bold"
            android:textSize="14dp"
            android:textColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintTop_toBottomOf="@id/tv_my_neighborhood_life"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <ImageView
                android:id="@+id/ic_daangen_storyarticle"
                android:layout_width="21dp"
                android:layout_height="21dp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="13dp"
                android:src="@drawable/ic_mydaangn_storyarticle_outline_24"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/tv_my_neighborhood_life_comment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14dp"
                android:text="동네생활 글/댓글"
                android:textColor="@color/black"
                app:layout_constraintStart_toEndOf="@id/ic_daangen_storyarticle"
                app:layout_constraintTop_toTopOf="@id/ic_daangen_storyarticle"
                android:layout_marginTop="-2dp"
                android:layout_marginStart="5dp"/>



        </androidx.constraintlayout.widget.ConstraintLayout>
        
    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/line_neighborhood_life"
        android:layout_width="wrap_content"
        android:layout_height="2dp"
        android:background="@color/white_gray"
        app:layout_constraintTop_toBottomOf="@id/layout_neighborhood_life"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="동네 가게"
        android:textColor="@color/black"
        android:fontFamily="@font/inter_bold"
        app:layout_constraintTop_toTopOf="@id/line_neighborhood_life"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="14dp"
        android:layout_marginStart="15dp"/>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/navigation"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:backgroundTint="@color/white"
        android:outlineSpotShadowColor="@color/black"
        app:menu="@menu/bottom_navigation_items"
        app:itemActiveIndicatorStyle="@null"
        app:itemBackground="@color/white"
        app:labelVisibilityMode="labeled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>



</androidx.constraintlayout.widget.ConstraintLayout>