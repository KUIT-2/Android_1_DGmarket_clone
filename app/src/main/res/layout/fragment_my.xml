<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/setting_mp"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="13dp"
        android:src="@drawable/ic_mydaangn_setting_outline_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/iv_user_profile_image"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="60dp"
        android:backgroundTint="@color/gray4"
        app:cardCornerRadius="100dp"
        app:cardElevation="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:fontFamily="@font/inter_bold"
        android:text="로그인을 해주세요"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_user_profile_image"
        app:layout_constraintStart_toEndOf="@+id/iv_user_profile_image"
        app:layout_constraintTop_toTopOf="@+id/iv_user_profile_image" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/login_btn"
        android:layout_width="80dp"
        android:layout_height="29dp"
        android:layout_alignParentRight="true"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="16dp"
        android:background="@color/gray5"
        android:clipToOutline="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <!-- Auto layout, variables, and unit scale are not yet supported -->
        <TextView
            android:id="@+id/tv_login"
            android:layout_width="54dp"
            android:layout_height="13dp"
            android:fontFamily="@font/inter_bold"
            android:gravity="top"
            android:text="@string/some_id"
            android:textAppearance="@style/some_id"
            android:textColor="@color/black"
            android:paddingStart="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/account_mp"
        android:layout_width="match_parent"
        android:layout_height="106dp"
        app:layout_constraintTop_toBottomOf="@id/iv_user_profile_image"
        android:layout_marginTop="19dp"
        android:layout_marginStart="14dp"
        android:layout_marginEnd="15dp">

        <ImageView
            android:id="@+id/DG_logo_mp"
            android:layout_width="56dp"
            android:layout_height="14dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="25dp"
            android:src="@drawable/onboarding_logo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/account_mp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="top"
            android:text="@string/DG_pay"
            android:textSize="13sp"
            android:layout_marginStart="14dp"
            app:layout_constraintStart_toEndOf="@+id/DG_logo_mp"
            app:layout_constraintTop_toTopOf="@+id/DG_logo_mp"/>

        <ImageView
            android:id="@+id/icon_post_a_mp"
            android:src="@drawable/icon_post_article_more"
            android:layout_width="9dp"
            android:layout_height="15dp"
            android:layout_marginTop="24dp"
            android:layout_marginRight="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <LinearLayout
            android:id="@+id/rectangle_3"
            android:layout_width="143dp"
            android:layout_height="33dp"
            android:layout_marginTop="19dp"
            android:background="@color/gray5"
            app:layout_constraintStart_toStartOf="@+id/DG_logo_mp"
            app:layout_constraintTop_toBottomOf="@+id/DG_logo_mp">
            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@drawable/tab_icon_write_gb_n"
                android:layout_marginTop="9dp"
                android:layout_marginStart="50dp"/>
            <TextView
                android:layout_width="24dp"
                android:layout_height="wrap_content"
                android:text="@string/charge"
                android:textSize="13sp"
                android:textColor="@color/black"
                android:fontFamily="@font/inter_bold"
                android:layout_gravity="center"
                android:layout_marginBottom="1dp"
                android:gravity="top"
                android:layout_marginStart="3dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/rectangle_4"
            android:layout_width="143dp"
            android:layout_height="33dp"
            android:layout_marginTop="19dp"
            android:layout_marginStart="9dp"
            android:background="@color/gray5"
            app:layout_constraintStart_toEndOf="@+id/rectangle_3"
            app:layout_constraintTop_toBottomOf="@+id/DG_logo_mp">
            <ImageView
                android:id="@+id/tab_icon_wr"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@drawable/tab_icon_write_gb_n"
                android:layout_marginTop="9dp"
                android:layout_marginStart="42dp"/>
            <TextView
                android:id="@+id/charge_mp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/remittance"
                android:textSize="13sp"
                android:textColor="@color/black"
                android:fontFamily="@font/inter_bold"
                android:layout_gravity="center"
                android:layout_marginStart="3dp"
                android:layout_marginBottom="1dp"
                android:gravity="top"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
        android:id="@+id/tv_recent_visit_mp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/account_mp"
        app:layout_constraintStart_toStartOf="parent"
        android:text="최근 방문"
        android:textColor="@color/black"
        android:fontFamily="@font/inter_bold"/>
    <LinearLayout
        android:id="@+id/layout_recent_list_mp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/tv_recent_visit_mp"
        app:layout_constraintStart_toStartOf="parent">
    <androidx.cardview.widget.CardView
        android:id="@+id/whole_mp"
        android:layout_width="68dp"
        android:layout_height="34dp"
        android:layout_marginBottom="1dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="9dp"
        app:cardCornerRadius="100dp">
        <ImageView
            android:id="@+id/tab_icon_ca"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:background="@drawable/tab_icon_category_n"
            android:layout_marginStart="14dp"
            android:layout_marginTop="9dp"/>
        <TextView
            android:layout_width="23dp"
            android:layout_height="15dp"
            android:text="@string/whole_menu"
            android:textAppearance="@style/some_id"
            android:gravity="top"
            android:layout_marginStart="33dp"
            android:layout_marginTop="9dp"/>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/man_fashion_mp"
        android:layout_width="99dp"
        android:layout_height="33dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="9dp"
        app:cardCornerRadius="100dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="15dp"
            android:text="@string/man_fashion"
            android:textAppearance="@style/some_id"
            android:gravity="top"
            android:layout_marginStart="14dp"
            android:layout_marginTop="9dp"/>
    </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/woman_clothes_mp"
            android:layout_width="73dp"
            android:layout_height="33dp"
            android:layout_marginStart="9dp"
            android:layout_marginTop="9dp"
            app:cardCornerRadius="100dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:text="@string/woman_clothes"
                android:textAppearance="@style/some_id"
                android:gravity="top"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="14dp"
                android:layout_marginTop="9dp"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/furniture_interior_mp"
            android:layout_width="99dp"
            android:layout_height="33dp"
            android:layout_marginStart="7dp"
            android:layout_marginTop="9dp"
            app:cardCornerRadius="100dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:text="@string/furniture_interior"
                android:textAppearance="@style/some_id"
                android:gravity="top"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="14dp"
                android:layout_marginTop="9dp"/>
        </androidx.cardview.widget.CardView>
    </LinearLayout>
    <TextView
        android:id="@+id/my_exchange_mp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/my_exchange"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:fontFamily="@font/inter_bold"
        android:layout_marginStart="15dp"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/layout_recent_list_mp"
        app:layout_constraintStart_toStartOf="parent"/>
    <ImageView
        android:id="@+id/ic_feed_int"
        android:layout_width="21dp"
        android:layout_height="18dp"
        android:src="@drawable/ic_feed_interest_outline_18"
        android:layout_marginStart="16dp"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/my_exchange_mp"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/interest_list_mp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/interest_list"
        android:textSize="14sp"
        android:gravity="top"
        android:textColor="@color/black"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toEndOf="@+id/ic_feed_int"
        app:layout_constraintTop_toTopOf="@+id/ic_feed_int"/>
    <ImageView
        android:id="@+id/ic_communit"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:src="@drawable/ic_community_outline_m"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/ic_feed_int"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/sold_list_mp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sold_list"
        android:textSize="14sp"
        android:gravity="top"
        android:textColor="@color/black"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toEndOf="@+id/ic_communit"
        app:layout_constraintTop_toTopOf="@+id/ic_communit"/>

    <ImageView
        android:id="@+id/ic_voucher"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:src="@drawable/ic_voucher_outline_m"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/ic_communit"
        app:layout_constraintStart_toStartOf="parent" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/purchased_list"
        android:textSize="14sp"
        android:gravity="top"
        android:textColor="@color/black"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toEndOf="@+id/ic_voucher"
        app:layout_constraintTop_toTopOf="@+id/ic_voucher"/>

    <ImageView
        android:id="@+id/btn_favorit"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:src="@drawable/btn_favorite_text_for_post"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/ic_voucher"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/financial_ledge"
        android:textSize="14sp"
        android:gravity="top"
        android:textColor="@color/black"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toEndOf="@+id/btn_favorit"
        app:layout_constraintTop_toTopOf="@+id/btn_favorit"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray6"
        android:layout_marginTop="22dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_favorit"/>
    <TextView
        android:id="@+id/my_life_mp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/my_life"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:fontFamily="@font/inter_bold"
        android:layout_marginTop="42dp"
        android:layout_marginStart="15dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_favorit"
        app:layout_constraintStart_toStartOf="parent"/>

    <ImageView
        android:id="@+id/ic_mydaangn"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:background="@drawable/ic_mydaangn_storyarticle_outline_24"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/my_life_mp"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/post_comment"
        android:textSize="14sp"
        android:gravity="top"
        android:textColor="@color/black"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toEndOf="@+id/ic_mydaangn"
        app:layout_constraintTop_toTopOf="@+id/ic_mydaangn"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray6"
        android:layout_marginTop="22dp"
        app:layout_constraintTop_toBottomOf="@+id/ic_mydaangn"/>
    <TextView
        android:id="@+id/town_store_mp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/town_store"
        android:textSize="14sp"
        android:textColor="@color/black"
        android:fontFamily="@font/inter_bold"
        android:layout_marginTop="39dp"
        android:layout_marginStart="15dp"
        app:layout_constraintTop_toBottomOf="@+id/ic_mydaangn"
        app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>